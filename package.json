{
  "name": "iobroker.tractive-gps",
  "version": "0.1.1",
  "description": "This adapter allows you to connect to the Tractive GPS service and retrieve the location of your pets.",
  "author": {
    "name": "xXBJXx",
    "email": "issi.dev.iobroker@gmail.com"
  },
  "homepage": "https://github.com/xXBJXx/ioBroker.tractive-gps",
  "license": "MIT",
  "keywords": [
    "tractive",
    "gps",
    "geoposition",
    "geolocation",
    "tracking",
    "dog",
    "cat",
    "pet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xXBJXx/ioBroker.tractive-gps.git"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.7",
    "axios": "^1.3.4",
    "cron": "^2.2.0",
    "dayjs": "^1.11.7",
    "react-leaflet": "3.2.5",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.9",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/testing": "^4.1.0",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@mui/styles": "^5.11.9",
    "@tsconfig/node16": "^1.0.3",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cron": "^2.0.0",
    "@types/leaflet": "^1.9.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.1",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "iobroker-react": "git+https://git@github.com/xXBJXx/iobroker-react.git#IssiVersion",
    "leaflet": "^1.9.3",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "rimraf": "^4.1.2",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/xXBJXx/ioBroker.tractive-gps/issues"
  },
  "readmeFilename": "README.md"
}
